{
  "name": "liya",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "start:ssr": "cross-env NODE_ENV='development' nodemon ./server/index.ts",
    "watch": "pnpm --parallel --filter=@liya/* --filter=!@liya/*-pages watch",
    "clean": "pnpm --filter=@liya/* clean && rimraf node_modules",
    "start:h5": "pnpm --filter=@liya/h5-pages start",
    "build:h5": "pnpm --filter @liya/h5-pages build",
    "start:rn": "pnpm --filter=@liya/rn-pages start",
    "start:ios": "pnpm --filter=@liya/rn-pages ios",
    "start:android": "pnpm --filter=@liya/rn-pages android",
    "test": "jest",
    "prepare": "is-ci || husky install"
  },
  "husky": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "git add ."
    ]
  },
  "workspaces": [
    "packages/*",
    "projects/*"
  ],
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "@reduxjs/toolkit": "^1.8.3",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "connect-history-api-fallback": "^2.0.0",
    "cookie-parser": "^1.4.6",
    "immer": "^9.0.15",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.3",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-router-native": "^6.3.0",
    "redux": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@expo/metro-config": "^0.3.21",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/body-parser": "^1.19.2",
    "@types/connect-history-api-fallback": "^1.3.5",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/glob": "^7.2.0",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-native": "^0.69.3",
    "@types/webp-in-css": "^0.7.0",
    "@types/webpack-node-externals": "^2.5.3",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "expo": "46.0.2",
    "express": "^4.18.1",
    "glob": "^8.0.3",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jest": "^28.1.3",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "nodemon": "^2.0.19",
    "only-allow": "^1.1.1",
    "open": "^8.4.0",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.7.2",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.3",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "typescript-plugin-css-modules": "^3.4.0",
    "webp-in-css": "^0.7.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-middleware": "^5.3.3",
    "webpack-dev-server": "^4.9.3",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-node-externals": "^3.0.0"
  }
}
